
    <h1>Hogwarts Students aka Hoggers</h1>

    <% for student in @students %>
        <p>Name: <%= student.pretty_name() %> </p>
        <p>House: <%= student.house.name %> </p>
        <p>Age: <%= student.age %></p>
        <a href="/students/<%= student.id %>">Show</a>
        <hr>
    <% end %>


